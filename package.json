{
  "name": "data-exchange-service",
  "version": "1.0.0",
  "description": "Data exchange service rest api",
  "main": "index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start": "node ./dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "copyFiles": "copy-files-from-to",
    "build:clean": "rimraf ./dist --glob",
    "build": "yarn build:clean && tsc -p .",
    "start:test-server": "yarn build && yarn copyFiles && yarn start",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.test.json' mocha --recursive --file 'tests/index.ts' --require ts-node/register 'tests/**/*.spec.ts' --exit",
    "test2": "mocha './dist/tests/**/*.js'",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "npm run prettier && eslint src/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.1",
    "cookie-parser": "1.4.6",
    "dotenv": "16.4.5",
    "express": "4.21.0",
    "express-async-errors": "3.1.1",
    "http-errors": "2.0.0",
    "joi": "17.13.3",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.2",
    "lodash.isempty": "4.4.0",
    "lodash.omit": "4.5.0",
    "mongoose": "8.7.1",
    "morgan": "1.10.0",
    "short-unique-id": "5.2.0",
    "uuid": "10.0.0",
    "winston": "3.15.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/chai": "5.0.1",
    "@types/cookie-parser": "1.4.7",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.0",
    "@types/http-errors": "2.0.4",
    "@types/js-yaml": "4.0.9",
    "@types/jsonwebtoken": "9.0.7",
    "@types/lodash.isempty": "4.4.9",
    "@types/lodash.omit": "4.5.9",
    "@types/mocha": "10.0.9",
    "@types/morgan": "1.9.9",
    "@types/node": "22.7.5",
    "@types/supertest": "6.0.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@types/uuid": "10.0.0",
    "@types/yamljs": "0.2.34",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "chai": "5.1.2",
    "copy-files-from-to": "3.11.0",
    "cross-env": "7.0.3",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jsdom": "25.0.1",
    "jsdom-global": "3.0.2",
    "mocha": "10.8.2",
    "nodemon": "3.1.7",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "should": "13.2.3",
    "supertest": "7.0.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.2",
